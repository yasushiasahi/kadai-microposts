<% if users.any? %>
  <ul class="media-list">
    <% users.each do |user| %>
      <% unless user == current_user %>
        <li class="media">
          <div class="media-left">
            <img class="media-object img-rounded" src="<%= gravatar_url(user, { size: 50 }) %>" alt="">
          </div>
          <div class="media-body">
            <div>
              <%= user.nickname %>
            </div>
            <div>
              <p><%= link_to "View Profile", user_path(user) %></p>
            </div>
          </div>
        </li>
      <% end %>
    <% end %>
  </ul>
  <%= paginate users %>
<% end %>